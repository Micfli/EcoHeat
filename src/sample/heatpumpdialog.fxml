<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.DialogPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.text.Text?>

<DialogPane prefHeight="204.0" prefWidth="285.0" xmlns="http://javafx.com/javafx/11" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.HeatPumpDiallogController">
    <header>
        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Enter all informations bellow to add a new heat pump: " />
    </header>
    <content>
        <GridPane alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="108.0" prefWidth="354.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints vgrow="SOMETIMES" />
                <RowConstraints vgrow="SOMETIMES" />
                <RowConstraints vgrow="SOMETIMES" />
                <RowConstraints vgrow="SOMETIMES" />
            <RowConstraints />
            <RowConstraints />
            <RowConstraints />
            </rowConstraints>
            <children>
                <Label text="Enter manufacturer:" />
                <Label text="Enter model:" GridPane.rowIndex="1" />
                <Label text="Enter power in kW:" GridPane.rowIndex="2" />
                <Label text="Enter Te in °C:" GridPane.rowIndex="3" />
                <Label text="Enter Tw max in °C:" GridPane.rowIndex="4" />
                <Label text="Enter COP" GridPane.rowIndex="5" />
                <Label text="Enter price:" GridPane.rowIndex="6" />
                <TextField fx:id="manufacturerField" GridPane.columnIndex="1" />
                <TextField fx:id="modelField" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                <TextField fx:id="powerField" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                <TextField fx:id="teField" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                <TextField fx:id="twmaxField" GridPane.columnIndex="1" GridPane.rowIndex="4" />
                <TextField fx:id="copField" GridPane.columnIndex="1" GridPane.rowIndex="5" />
                <TextField fx:id="priceField" GridPane.columnIndex="1" GridPane.rowIndex="6" />
            </children>
        </GridPane>
    </content>
</DialogPane>
